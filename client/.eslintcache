{ Component } from 'react'\nimport MailService from '../../../service/email.service'\nimport FilesService from '../../../service/upload.service'\nimport Spinner from '../../shared/spinner/Loader'\nimport { Container, Form, Button, Col, Row} from 'react-bootstrap'\nimport Alert from '../../shared/Alert/Alert'\n\nclass ContactForm extends Component {\n    constructor(){\n        super()\n        this.state= {\n                \n            showToast: false,\n            toastText: '',\n            name: '',\n            email:'',\n            subject:'',\n            message:'',\n            sent: false\n            \n        }\n        this.MailService= new MailService()\n    }\n\n\n\n    handleInputChange = e => this.setState({ ...this.state,[e.target.name]: e.target.value} )\n\n    handleSubmit = e => {\n        e.preventDefault()\n        \n        let data= {\n            name:this.state.name,\n            email: this.state.email,\n            subject: this.state.subject,\n            message: this.state.message\n        }\n\n        this.MailService  \n            .sendMail(data)\n            .then(res => {\n                this.setState({\n                    sent:true,\n                    toastText:\"Email sent successfully!\",\n                    showToast: true\n                },this.resetForm())\n            })\n            .catch(() => {\n                this.setState({\n                    toastText:\"Error while sending the email, please try agian \",\n                    showToast: true\n                })\n            })\n    }\n\n    resetForm = () => {\n        this.setState({\n            name: '',\n            email:'',\n            subject:'',\n            message:'',\n            sent: false\n        })\n    }\n\n    handleToast= (visible, text) => this.setState({showToast: visible, toastText: text})\n\n\n    render(){\n        return (\n            <>\n            <Container style={{margin: '60px'}}>\n            <Row>\n                <Col md={{ span: 6, offset: 3 }} >\n                <h2 style={{display:'flex', justifyContent: 'center' }}>Contact Us</h2>\n                <Form onSubmit={this.handleSubmit}>\n                    <Form.Group controlId=\"name\">\n                    <Form.Label>Name:</Form.Label>\n                    <Form.Control type=\"text\" placeholder=\"Enter your name\" onChange={this.handleInputChange} name=\"name\" value={this.state.name}/>\n                </Form.Group>\n                <Form.Group controlId=\"description\">\n                    <Form.Label>Email:</Form.Label>\n                    <Form.Control type=\"text\" placeholder=\"Enter your email\" onChange={this.handleInputChange} name=\"email\" value={this.state.email} />\n                </Form.Group>\n                <Form.Group controlId=\"instructor\">\n                    <Form.Label>Subject:</Form.Label>\n                    <Form.Control type=\"text\" placeholder=\"Enter subject\" onChange={this.handleInputChange} name=\"subject\" value={this.state.subject} />\n                </Form.Group>\n                <Form.Group controlId=\"capacity\">\n                    <Form.Label>Message:</Form.Label>\n                     <Form.Control as=\"textarea\" rows={3} type=\"text\" value={this.state.message} onChange={this.handleInputChange}  name=\"message\"/>\n                </Form.Group>\n                <Button variant=\"primary\" type=\"submit\"> Submit</Button>\n                </Form>\n                </Col>\n            </Row>\n                </Container>\n\n                <Alert show={this.state.showToast} handleToast = {this.handleToast} toastText={this.state.toastText}/>\n            </>\n        )\n    }\n    \n}\n\nexport default ContactForm","/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/service/email.service.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/pages/Activity-details/Activity-details-popup.js",["152","153","154","155"],"import React from 'react'\nimport GymService from '../../../service/gym.service'\nimport MailService from '../../../service/email.service'\nimport './Activity-details.css'\nimport Alert from '../../shared/Alert/Alert'\nimport Loader from '../../shared/spinner/Loader'\n\nimport { Button} from 'react-bootstrap'\n\n\n\nconst ActivityDetails = ({activityDetailsInfo}) => {\n    const mailService = new MailService()\n    let toastText = ''\n    let showToast = false\n    const handleSubmit = e => {\n        e.preventDefault()\n        \n        let data= {\n            name: activityDetailsInfo.user.name,\n            email: activityDetailsInfo.user.email,\n            subject: `You have signed up for ${activityDetailsInfo.name}`,\n            message: `Thank you for signing up to ${activityDetailsInfo.name}! We will see you in class`\n        }\n\n        mailService  \n            .activityMail(data)\n            .then(() => toastText=\"Email sent successfully!\", showToast = true)\n            .catch(() => toastText=\"Error while signing you up, please try agian or contact the team\", showToast= true )\n    }\n\n    const gymService = new GymService()\n\n    const signClass = () => {\n        \n        console.log('User ID: ', activityDetailsInfo.user._id)\n        gymService\n        .joinClass(activityDetailsInfo.user._id, activityDetailsInfo._id)\n        .then(() => console.log('sign up complete!'))\n        .catch(err => console.log(err))\n    }\n\n//    const handleToast = (visible, text) => {\n//         showToast = visible,\n//         toastText = text\n//     }\n        \n        return(\n            \n            <>\n            {activityDetailsInfo ? \n           \n        <>\n            <h1>{activityDetailsInfo.name}</h1> \n                    <img src={activityDetailsInfo.avatar} alt={activityDetailsInfo.name}/>\n                    <h3>Instructor: {activityDetailsInfo.instructor}</h3>\n                    <p>{activityDetailsInfo.description}</p>\n                    { activityDetailsInfo.user ?\n                    <Button className=\"btn btn-dark\" onClick={signClass, handleSubmit} >Join</Button>\n                    : <h3>Log in to sign up!</h3>}\n                </>\n            : <Loader />}\n            {console.log(activityDetailsInfo)}\n\n            </>\n            \n            \n           \n        )\n}\n\nexport default ActivityDetails\n","/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/pages/Activity-form/Activity-edit.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/pages/Activity-form/Activity-edit-popup.js",["156","157","158"],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/pages/Team/Team.js",["159","160","161","162"],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/store.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/reducers/index.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/chatbox/chat.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/actions/watson.js",["163"],"import {INPUT_SUCCESS, INPUT_FAIL, SESSION_SUCCESS, SESSION_FAIL, MESSAGE_FAIL, MESSAGE_SUCCESS} from './types'\nimport axios from \"axios\"\nexport const userMessage = (message) => async (dispatch) => {\n    try{\n        dispatch({type:INPUT_SUCCESS, payload:message})\n    }catch(err){\n        dispatch({type: INPUT_FAIL})\n    }\n}\n\nexport const createSession = () => async (dispatch) => {\n    try{\n        const res = await axios.get('/api/watson/session')\n        dispatch({type:SESSION_SUCCESS, payload: res.data})\n    }catch(err){\n        dispatch({type:SESSION_FAIL})\n    }\n}\n\nexport const sendMessage = message => async (dispatch) => {\n    try{\n        const body = {input: message}\n        const res = axios.post(\"/api/watson/message\", body)\n        dispatch({type:SESSION_SUCCESS, payload: (await res).data.output.generic[0].text})\n    }catch(err){\n        dispatch({type: MESSAGE_FAIL})\n    }\n}","/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/actions/types.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/reducers/watson.js",["164"],"import {INPUT_SUCCESS, INPUT_FAIL, SESSION_SUCCESS, SESSION_FAIL, MESSAGE_SUCCESS, MESSAGE_FAIL} from '../actions/types'\n\nconst initialState = {\n    messages: []\n}\n\nexport default(state=initialState, action) => {\n    const {type, payload} = action\n    let {messages} = state;\n\n    switch(type){\n        case INPUT_SUCCESS: \n            messages = [...messages, {message:payload, type:\"user\"}]\n            return{\n                ...state,\n                messages\n            }\n        case INPUT_FAIL:\n            return {\n                ...state\n            }\n        case SESSION_SUCCESS:\n                localStorage.setItem(\"session\", payload[\"session_id\"])\n            return{\n                ...state,\n            }\n        case SESSION_FAIL:\n            return {\n                ...state,\n            }\n            case MESSAGE_SUCCESS:\n                messages = [...messages, {message:payload, type:\"bot\"}]\n                return{\n                    ...state,\n                    messages\n                }\n            case MESSAGE_FAIL:\n                return{\n                    ...state\n                }\n        default:\n            return{\n                ...state,\n            }\n    }\n}",{"ruleId":"165","replacedBy":"166"},{"ruleId":"167","replacedBy":"168"},{"ruleId":"169","severity":1,"message":"170","line":4,"column":25,"nodeType":"171","messageId":"172","endLine":4,"endColumn":33},{"ruleId":"173","severity":1,"message":"174","line":23,"column":24,"nodeType":"175","messageId":"176","endLine":2