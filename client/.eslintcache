 extends Component {\n  constructor(){\n    super()\n    this.state = {\n      loggedInUser: undefined\n    }\n    this.AuthService = new AuthService\n  }\n\n  componentDidMount = () => {\n      this.AuthService\n      .isLoggedIn()\n      .then(res => this.setState({loggedInUser: res.data}))\n      .catch(err => console.log(err))\n  }\n  setTheUser = user => this.setState({ loggedInUser: user}, () => console.log('The new state is: ', this.state) )\n  \n  render(){\n  return (\n    <>\n    <Navigation storeUser={this.setTheUser} loggedUser={this.state.loggedInUser}/>\n    <main> \n    \n    <Switch>\n    <Route path='/' exact component={Home} />\n    <Route  path='/activities' exact render= { () => <ActivityList user={this.state.loggedInUser} />} />\n    <Route path='/activities/:activity_id' render= { props => <ActivityDetails {...props} user={this.state.loggedInUser}/> }/>\n    <Route path='/createactivity' render= { () => <ActivityForm />} />\n    <Route path='/signup' render= { props => <Signup storeUser={this.setTheUser}  {...props} />} />\n    <Route path='/login' render= { props => <Login storeUser={this.setTheUser} {...props} />} />\n    <Route path=\"/contact\" render={() => <ContactForm />} />\n    <Route path=\"/team\" render={() => <Team />} />\n    </Switch>\n    </main>\n    <Footer storeUser={this.setTheUser} loggedUser={this.state.loggedInUser}/>\n    </>\n  )\n  }\n}\n\nexport default App;\n","/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/pages/Activity-list/Activity-list.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/service/gym.service.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/pages/Activity-list/ActivityCard.js",[],"/Users/bitu/Desktop/ironhack/module-3/final-project-gym/final-project/client/src/components/pages/Activity-details/Activity-details.js",["146"],"import React, { Component } from 'react'\nimp